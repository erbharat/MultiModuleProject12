                  <Button fx:id="signUpBtn" disable="false" layoutX="30.0" mnemonicParsing="false" onAction="#signUp" prefHeight="30.0" prefWidth="75.0" style="-fx-background-color: deepskyblue;" text="Sign up" />
                  <Button fx:id="logoutBtn" layoutX="262.5" mnemonicParsing="false" onAction="#logout" prefHeight="30.0" prefWidth="75.0" style="-fx-background-color: deepskyblue;" text="Logout" visible="false" />
                  <Button fx:id="homeBtn" layoutX="495.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="75.0" style="-fx-background-color: deepskyblue;" text="Login" />
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</VBox>
