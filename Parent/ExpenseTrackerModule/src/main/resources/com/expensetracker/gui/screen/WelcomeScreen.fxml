              			<MenuItem fx:id="closeMenuItem" mnemonicParsing="false" text="Close" />
            		</items>
          		</Menu>
         		<Menu id="profileMenu" mnemonicParsing="false" text="Profile">
            		<items>
              			<MenuItem fx:id="deleteMenuItem" mnemonicParsing="false" text="Delete" />
            		</items>
          		</Menu>
          		<Menu id="helpMenu" mnemonicParsing="false" text="Help">
            		<items>
              			<MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="About" />
            		</items>
          		</Menu>
              </menus>
            </MenuBar>
         </children>
      </AnchorPane>
      <StackPane fx:id="screenSwitcher" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="340.0" prefWidth="600.0" VBox.vgrow="ALWAYS" />
      <AnchorPane prefHeight="30.0" prefWidth="600.0" style="-fx-background-color: skyblue;">
         <children>
            <Pane layoutX="25.0" layoutY="2.0" prefHeight="31.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
